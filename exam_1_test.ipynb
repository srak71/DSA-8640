    "# Question 23\n",
    "def fib(n):\n",
    "    a = 1\n",
    "    b = 1\n",
    "    c = a + b\n",
    "    seq = [1, 1]\n",
    "    \n",
    "    while c < n:\n",
    "        seq.append(c)\n",
    "        a = c\n",
    "        b = seq[-2]\n",
    "        \n",
    "        c = a + b\n",
    "        \n",
    "    for i in seq:\n",
    "        print(i, end = ' ')\n",
    "\n",
    "\n",
    "fib(12)\n",
    "print('\\n')\n",
    "fib(25)"
   ]
